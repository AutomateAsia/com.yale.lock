{
  "id": "com.yale.lock",
  "sdk": 2,
  "name": {
    "en": "Yale Digital Lock"
  },
  "description": {
    "en": "Adds support for Yale Digital Locks"
  },
  "category": "security",
  "version": "1.0.3",
  "compatibility": ">=1.5.0",
  "author": {
    "name": "Mike Lim",
    "email": "mike.lim@automate.asia"
  },
  "images": {
    "large": "/assets/images/large.jpg",
    "small": "/assets/images/small.jpg"
  },
  "flow": {
    "triggers": [
      {
        "id": "homey_unlocked",
        "title": {
          "en": "Unlocked by Z-Wave"
        },
        "args": [
          {
            "name": "device",
            "type": "device",
            "filter": "driver_id=YRD220|YDD424|YFM40|YDM3168|YKFCON"
          }
        ]
      },
      {
        "id": "homey_locked",
        "title": {
          "en": "Locked by Z-Wave"
        },
        "args": [
          {
            "name": "device",
            "type": "device",
            "filter": "driver_id=YRD220|YDD424|YFM40|YDM3168|YKFCON"
          }
        ]
      },
      {
        "id": "manual_unlocked",
        "title": {
          "en": "Unlocked by thumbturn"
        },
        "args": [
          {
            "name": "device",
            "type": "device",
            "filter": "driver_id=YRD220|YDD424|YFM40|YDM3168|YKFCON"
          }
        ]
      },
      {
        "id": "touchpad_unlocked",
        "title": {
          "en": "Unlocked by touchpad"
        },
        "args": [
          {
            "name": "device",
            "type": "device",
            "filter": "driver_id=YRD220|YDD424|YFM40|YDM3168|YKFCON"
          }
        ]
      },
      {
        "id": "user_unlocked",
        "title": {
          "en": "Unlocked by user"
        },
        "tokens": [
          {
            "name": "userid",
            "type": "number",
            "title": {
              "en": "User ID"
            },
            "example": {
              "en": "1"
            }
          }
        ],
        "args": [
          {
            "name": "device",
            "type": "device",
            "filter": "driver_id=YRD220|YDD424|YFM40|YDM3168|YKFCON"
          }
        ]
      },
      {
        "id": "manual_locked",
        "title": {
          "en": "Locked by thumbturn"
        },
        "args": [
          {
            "name": "device",
            "type": "device",
            "filter": "driver_id=YRD220|YDD424|YFM40|YDM3168|YKFCON"
          }
        ]
      },
      {
        "id": "touchpad_locked",
        "title": {
          "en": "Locked by touchpad"
        },
        "args": [
          {
            "name": "device",
            "type": "device",
            "filter": "driver_id=YRD220|YDD424|YFM40|YDM3168|YKFCON"
          }
        ]
      },
      {
        "id": "button_unlocked",
        "title": {
          "en": "Unlocked by button"
        },
        "args": [
          {
            "name": "device",
            "type": "device",
            "filter": "driver_id=YDD424"
          }
        ]
      },
      {
        "id": "button_locked",
        "title": {
          "en": "Locked by button"
        },
        "args": [
          {
            "name": "device",
            "type": "device",
            "filter": "driver_id=YDD424"
          }
        ]
      },
      {
        "id": "auto_locked",
        "title": {
          "en": "Auto Locked"
        },
        "args": [
          {
            "name": "device",
            "type": "device",
            "filter": "driver_id=YDD424|YFM40|YDM3168"
          }
        ]
      },
      {
        "id": "tamper_alarm",
        "title": {
          "en": "The tamper alarm is set off"
        },
        "args": [
          {
            "name": "device",
            "type": "device",
            "filter": "driver_id=YRD220|YDD424|YFM40|YDM3168"
          },
          {
            "name": "alarmtype",
            "type": "dropdown",
            "values": [
              {
                "id": "1",
                "label": {
                  "en": "Too many keypad attempts"
                }
              },
              {
                "id": "2",
                "label": {
                  "en": "Lock damaged"
                }
              }
            ]
          }
        ]
      }
    ]
  },
